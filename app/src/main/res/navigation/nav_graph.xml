<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/vijestiNavDrawer">

    <fragment
        android:id="@+id/infoBottomNav"
        android:name="Portal.fragmenti.fragmenti.InfoFragment"
        android:label="Info"
        tools:layout="@layout/info_fragment"/>
    <fragment
        android:id="@+id/kontaktBottomNav"
        android:name="Portal.fragmenti.fragmenti.KontaktFragment"
        android:label="Kontakt"
        tools:layout="@layout/kontakt_fragment"/>
    <fragment
        android:id="@+id/naslovnicaBottomNav"
        android:name="Portal.fragmenti.fragmenti.NaslovnicaFragment"
        android:label="Naslovnica"
        tools:layout="@layout/naslovnica_fragment"/>
    <fragment
        android:id="@+id/obavijestiNavDrawer"
        android:name="Portal.fragmenti.fragmenti.ObavijestiFragment"
        android:label="Obavijesti"
        tools:layout="@layout/obavijesti_fragment">
        <action
            android:id="@+id/action_obavijestiNavDrawer_to_updateDeleteObavijestiFragment2"
            app:destination="@id/updateDeleteObavijestiFragment2" />
        <action
            android:id="@+id/action_obavijestiNavDrawer_to_detailObavijestiFragment"
            app:destination="@id/detailObavijestiFragment" />
    </fragment>
    <fragment
        android:id="@+id/oglasnikNavDrawer"
        android:name="Portal.fragmenti.fragmenti.OglasnikFragment"
        android:label="Oglasnik"
        tools:layout="@layout/oglasnik_fragment">
        <action
            android:id="@+id/action_oglasnikNavDrawer_to_updateDeleteOglasnikFragment"
            app:destination="@id/updateDeleteOglasnikFragment" />
        <action
            android:id="@+id/action_oglasnikNavDrawer_to_detailOglasnikFragment"
            app:destination="@id/detailOglasnikFragment" />
    </fragment>
    <fragment
        android:id="@+id/priceCitateljaNavDrawer"
        android:name="Portal.fragmenti.fragmenti.PriceCitateljaFragment"
        android:label="Priče čitatelja"
        tools:layout="@layout/price_citatelja_fragment">
        <action
            android:id="@+id/action_priceCitateljaNavDrawer_to_detailPriceCitateljaFragment"
            app:destination="@id/detailPriceCitateljaFragment" />
        <action
            android:id="@+id/action_priceCitateljaNavDrawer_to_updateDeletePriceCitateljaFragment"
            app:destination="@id/updateDeletePriceCitateljaFragment" />
    </fragment>
    <fragment
        android:id="@+id/sportNavDrawer"
        android:name="Portal.fragmenti.fragmenti.SportFragment"
        android:label="Sport"
        tools:layout="@layout/sport_fragment">
        <action
            android:id="@+id/action_sportNavDrawer_to_updateDeleteSportFragment"
            app:destination="@id/updateDeleteSportFragment" />
        <action
            android:id="@+id/action_sportNavDrawer_to_detailSportFragment"
            app:destination="@id/detailSportFragment" />
    </fragment>
    <fragment
        android:id="@+id/vijestiNavDrawer"
        android:name="Portal.fragmenti.fragmenti.VijestiFragment"
        android:label="Vijesti"
        tools:layout="@layout/vijesti_fragment">
        <action
            android:id="@+id/action_vijestiNavDrawer_to_updateDeleteVijestiFragment"
            app:destination="@id/updateDeleteVijestiFragment" />
        <action
            android:id="@+id/action_vijestiNavDrawer_to_detailVijestiFragment"
            app:destination="@id/detailVijestiFragment" />
    </fragment>
    <fragment
        android:id="@+id/zabavaNavDrawer"
        android:name="Portal.fragmenti.fragmenti.ZabavaFragment"
        android:label="Zabava"
        tools:layout="@layout/zabava_fragment">
        <action
            android:id="@+id/action_zabavaNavDrawer_to_updateDeleteZabavaFragment"
            app:destination="@id/updateDeleteZabavaFragment" />
        <action
            android:id="@+id/action_zabavaNavDrawer_to_detailFragmentZabava"
            app:destination="@id/detailFragmentZabava" />
    </fragment>
    <fragment
        android:id="@+id/vrijemeBottomNav"
        android:name="Portal.fragmenti.fragmenti.VrijemeFragment"
        android:label="Vrijeme"
        tools:layout="@layout/vrijeme_fragment"/>
    <fragment
        android:id="@+id/menu_dodajNovuVijest"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoVijesti"
        android:label="Dodaj novu vijesti"
        tools:layout="@layout/dodaj_novo_vijesti_fragment">
        <action
            android:id="@+id/action_menu_dodajNovuVijest_to_vijestiNavDrawer"
            app:destination="@id/vijestiNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/menu_dodajNovuObavijest"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoObavijesti"
        android:label="Dodaj novu obavijest"
        tools:layout="@layout/dodaj_novo_obavijesti_fragment">
        <action
            android:id="@+id/action_menu_dodajNovuObavijest_to_obavijestiNavDrawer"
            app:destination="@id/obavijestiNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/menu_dodajNoviSport"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoSport"
        android:label="Dodaj novi sport"
        tools:layout="@layout/dodaj_novo_sport_fragment">
        <action
            android:id="@+id/action_menu_dodajNoviSport_to_sportNavDrawer"
            app:destination="@id/sportNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/menu_dodajNovuZabava"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoZabava"
        android:label="Dodaj novu zabavu"
        tools:layout="@layout/dodaj_novo_zabava_fragment">
        <action
            android:id="@+id/action_menu_dodajNovuZabava_to_zabavaNavDrawer"
            app:destination="@id/zabavaNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/menu_dodajNoviOglas"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoOglasnik"
        android:label="Dodaj novi oglas"
        tools:layout="@layout/dodaj_novo_oglasnik_fragment">
        <action
            android:id="@+id/action_menu_dodajNoviOglas_to_oglasnikNavDrawer"
            app:destination="@id/oglasnikNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/updateDeleteObavijestiFragment2"
        android:name="Portal.fragmenti.updateDelete.UpdateDeleteObavijestiFragment"
        android:label="Ažuriraj ili obriši obavijest"
        tools:layout="@layout/update_delete_obavijesti_fragment">
        <action
            android:id="@+id/action_updateDeleteObavijestiFragment2_to_obavijestiNavDrawer"
            app:destination="@id/obavijestiNavDrawer" />
        <argument
            android:name="currentObavijest"
            app:argType="Portal.database.table.ObavijestiTable" />
    </fragment>
    <fragment
        android:id="@+id/updateDeleteSportFragment"
        android:name="Portal.fragmenti.updateDelete.UpdateDeleteSportFragment"
        android:label="Ažuriraj ili obriši sport"
        tools:layout="@layout/update_delete_sport_fragment">
        <action
            android:id="@+id/action_updateDeleteSportFragment_to_sportNavDrawer"
            app:destination="@id/sportNavDrawer" />
        <argument
            android:name="currentSport"
            app:argType="Portal.database.table.SportTable" />
    </fragment>
    <fragment
        android:id="@+id/updateDeleteVijestiFragment"
        android:name="Portal.fragmenti.updateDelete.UpdateDeleteVijestiFragment"
        android:label="Ažuriraj ili obriši vijest"
        tools:layout="@layout/update_delete_vijesti_fragment">
        <action
            android:id="@+id/action_updateDeleteVijestiFragment_to_vijestiNavDrawer"
            app:destination="@id/vijestiNavDrawer" />
        <argument
            android:name="currentVijest"
            app:argType="Portal.database.table.VijestiTable" />
    </fragment>
    <fragment
        android:id="@+id/updateDeleteZabavaFragment"
        android:name="Portal.fragmenti.updateDelete.UpdateDeleteZabavaFragment"
        android:label="Ažuriraj ili obriši zabavu"
        tools:layout="@layout/update_delete_zabava_fragment">
        <action
            android:id="@+id/action_updateDeleteZabavaFragment_to_zabavaNavDrawer"
            app:destination="@id/zabavaNavDrawer" />
        <argument
            android:name="currentZabava"
            app:argType="Portal.database.table.ZabavaTable" />
    </fragment>
    <fragment
        android:id="@+id/updateDeleteOglasnikFragment"
        android:name="Portal.fragmenti.updateDelete.UpdateDeleteOglasnikFragment"
        android:label="Ažuriraj ili obriši oglas"
        tools:layout="@layout/update_delete_oglasnik_fragment">
        <action
            android:id="@+id/action_updateDeleteOglasnikFragment_to_oglasnikNavDrawer"
            app:destination="@id/oglasnikNavDrawer" />
        <argument
            android:name="currentOglasnik"
            app:argType="Portal.database.table.OglasnikTable" />
    </fragment>
    <fragment
        android:id="@+id/detailSportFragment"
        android:name="Portal.fragmenti.detailFragmenti.DetailSportFragment"
        android:label="Detalji sport"
        tools:layout="@layout/detail_sport_fragment">
        <argument
            android:name="sportData"
            app:argType="Portal.database.table.SportTable" />
    </fragment>
    <fragment
        android:id="@+id/detailFragmentZabava"
        android:name="Portal.fragmenti.detailFragmenti.DetailZabavaFragment"
        android:label="Detalji zabava"
        tools:layout="@layout/detail_zabava_fragment">
        <argument
            android:name="zabavaData"
            app:argType="Portal.database.table.ZabavaTable" />
    </fragment>
    <fragment
        android:id="@+id/detailObavijestiFragment"
        android:name="Portal.fragmenti.detailFragmenti.DetailObavijestiFragment"
        android:label="Detalji obavijest"
        tools:layout="@layout/detail_obavijesti_fragment">
        <argument
            android:name="obavijestiData"
            app:argType="Portal.database.table.ObavijestiTable" />
    </fragment>
    <fragment
        android:id="@+id/detailOglasnikFragment"
        android:name="Portal.fragmenti.detailFragmenti.DetailOglasnikFragment"
        android:label="Detalji oglas"
        tools:layout="@layout/detail_oglasnik_fragment">
        <argument
            android:name="oglasnikData"
            app:argType="Portal.database.table.OglasnikTable" />
    </fragment>
    <fragment
        android:id="@+id/detailVijestiFragment"
        android:name="Portal.fragmenti.detailFragmenti.DetailVijestiFragment"
        android:label="Detalji vijest"
        tools:layout="@layout/detail_vijesti_fragment">
        <argument
            android:name="vijestiData"
            app:argType="Portal.database.table.VijestiTable" />
    </fragment>
    <fragment
        android:id="@+id/menu_dodajNovuPricuCitatelja"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoPriceCitatelja"
        android:label="Dodaj novu priču čitatelja"
        tools:layout="@layout/dodaj_novo_price_citatelja_fragment">
        <action
            android:id="@+id/action_menu_dodajNovuPricuCitatelja_to_priceCitateljaNavDrawer"
            app:destination="@id/priceCitateljaNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/detailPriceCitateljaFragment"
        android:name="Portal.fragmenti.detailFragmenti.DetailPriceCitateljaFragment"
        android:label="Detalji priča čitatelja"
        tools:layout="@layout/detail_price_citatelja_fragment">
        <argument
            android:name="priceCitateljaData"
            app:argType="Portal.database.table.PriceCitateljaTable" />
    </fragment>
    <fragment
        android:id="@+id/updateDeletePriceCitateljaFragment"
        android:name="Portal.fragmenti.updateDelete.UpdateDeletePriceCitateljaFragment"
        android:label="Ažuriraj ili obriši priču čitatelja"
        tools:layout="@layout/update_delete_price_citatelja_fragment">
        <action
            android:id="@+id/action_updateDeletePriceCitateljaFragment_to_priceCitateljaNavDrawer"
            app:destination="@id/priceCitateljaNavDrawer" />
        <argument
            android:name="currentPriceCitatelja"
            app:argType="Portal.database.table.PriceCitateljaTable" />
    </fragment>
    <fragment
        android:id="@+id/detailPoljoprivredaFragment"
        android:name="Portal.fragmenti.detailFragmenti.DetailPoljoprivredaFragment"
        android:label="Detalji poljoprivreda"
        tools:layout="@layout/detail_poljoprivreda_fragment">
        <argument
            android:name="detailPoljoprivredaArgs"
            app:argType="Portal.database.table.PoljoprivredaTable" />
    </fragment>
    <fragment
        android:id="@+id/menu_dodajNovuPoljoprivredu"
        android:name="Portal.fragmenti.dodajNovo.DodajNovoPoljoprivreda"
        android:label="Dodaj novu poljoprivredu"
        tools:layout="@layout/dodaj_novo_poljoprivreda_fragment">
        <action
            android:id="@+id/action_menu_dodajNovuPoljoprivredu_to_poljoprivredaNavDrawer"
            app:destination="@id/poljoprivredaNavDrawer" />
    </fragment>
    <fragment
        android:id="@+id/poljoprivredaNavDrawer"
        android:name="Portal.fragmenti.fragmenti.PoljoprivredaFragment"
        android:label="Poljoprivreda"
        tools:layout="@layout/poljoprivreda_fragment">
        <action
            android:id="@+id/action_poljoprivredaNavDrawer_to_updateDeletePoljoprivredaFragment"
            app:destination="@id/updateDeletePoljoprivredaFragment" />
        <action
            android:id="@+id/action_poljoprivredaNavDrawer_to_detailPoljoprivredaFragment"
            app:destination="@id/detailPoljoprivredaFragment" />
    </fragment>
    <fragment
        android:id="@+id/updateDeletePoljoprivredaFragment"
        android:name="Portal.fragmenti.updateDelete.UpdateDeletePoljoprivredaFragment"
        android:label="Ažuriraj ili obiriši poljoprivredu"
        tools:layout="@layout/update_delete_poljoprivreda_fragment">
        <action
            android:id="@+id/action_updateDeletePoljoprivredaFragment_to_poljoprivredaNavDrawer"
            app:destination="@id/poljoprivredaNavDrawer" />
        <argument
            android:name="updatePoljoprivredaArgs"
            app:argType="Portal.database.table.PoljoprivredaTable" />
    </fragment>
    <fragment
        android:id="@+id/menu_admin"
        android:name="Portal.fragmenti.fragmenti.AdminFragment"
        android:label="Admin"
        tools:layout="@layout/admin_fragment"/>
</navigation>